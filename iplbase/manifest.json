{
  "manifest_version": 3,
  "name": "IPL Wiki Extension",
  "update_url": "http://www2.ipl.co.jp/iwai/wiki/iplbase.xml",
  "version": "3",
  "content_scripts": [
    {
      "matches": ["http://*.iplwiki.info/*","https://*.iplwiki.info/*"],
